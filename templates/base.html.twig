<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('build/shared-style.css') }}"/>
        {% block stylesheets_custom %}{% endblock %}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('shared-scripts') }}
